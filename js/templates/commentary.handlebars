{{#each comment}}
<div class="col-md-12 media" id="{{id_comment}}">
  <div class="media-body">
    <h4 class="media-heading title">{{name}}</h4>
    <h5 class="media-subheading points">Puntaje: {{score}}</h5>
    <p>{{comment}}</p>
    <a href="#" onclick="deleteCommentary({{id_comment}})"><i class="fa fa-trash fa-2x fa-fw" aria-hidden="true"></i></a>
  </div>
</div>
{{/each}}
